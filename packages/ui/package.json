{
  "name": "@benable/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "generate": "tamagui generate-themes ./src/tamagui/themes.ts ./src/generated-themes.ts",
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "1.68.5",
    "@tamagui/config": "1.68.5",
    "@tamagui/create-theme": "1.68.5",
    "@tamagui/font-inter": "1.68.5",
    "@tamagui/lucide-icons": "1.68.5",
    "@tamagui/react-native-media-driver": "1.68.5",
    "@tamagui/shorthands": "1.68.5",
    "@tamagui/theme-builder": "1.68.5",
    "@tamagui/themes": "1.68.5",
    "@tamagui/toast": "1.68.5",
    "@tamagui/web": "1.68.5",
    "tamagui": "1.68.5"
  },
  "devDependencies": {
    "@tamagui/build": "1.68.5",
    "@tamagui/cli": "1.68.5",
    "@tamagui/generate-themes": "1.68.5"
  }
}
